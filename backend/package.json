{
  "name": "backend",
  "version": "1.0.0",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "start": "ts-node src/index.ts",
    "docker": "dotenv -e .env -- npx nodemon -e .ts index.ts",
    "dev": "nodemon --exec ts-node src/index.ts",
    "init-db": "ts-node src/database-init.ts",
    "seed-db": "ts-node src/seed-database.ts",
    "force-reseed": "ts-node src/force-reseed.ts",
    "show-db": "ts-node src/show-database.ts",
    "cleanup-db": "ts-node -e \"import('./src/database-init').then(m => m.cleanupExpiredData())\"",
    "db-stats": "ts-node -e \"import('./src/database-init').then(m => m.getDatabaseStats().then(console.log))\"",
    "format": "prettier --check \"src/**/*.{ts,js,json}\"",
    "format:fix": "prettier --write \"src/**/*.{ts,js,json}\""
  },
  "author": "",
  "license": "ISC",
  "description": "TypeScript backend project",
  "dependencies": {
    "dotenv": "^17.2.1",
    "express": "^4.18.0",
    "google-auth-library": "^10.2.1",
    "googleapis": "^164.1.0",
    "jsonwebtoken": "^9.0.2",
    "mongoose": "^8.18.0",
    "multer": "^2.0.2",
    "socket.io": "^4.8.1",
    "zod": "^4.1.1"
  },
  "devDependencies": {
    "@types/express": "^4.17.0",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/multer": "^2.0.0",
    "@types/node": "^20.0.0",
    "nodemon": "^3.0.0",
    "prettier": "^3.6.2",
    "ts-node": "^10.9.0",
    "typescript": "^5.0.0"
  }
}
